    "patch-package": "^8.0.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "18.3.1",
    "release-it": "^17.10.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "overrides": {
    "@atlaskit/editor-bitbucket-transformer": {
      "react": "$react"
